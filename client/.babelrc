    "presets": [
        [
            "@babel/preset-typescript",
            {
                "isTSX": true,
                "allExtensions": true
            }
        ],
        [
            "@babel/preset-env",
            {
                "targets": "> 0.25%, not dead",
                "debug": true
            }
        ], "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ]
