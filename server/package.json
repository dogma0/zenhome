{
  "name": "typescript-graphql-auth",
  "version": "1.0.0",
  "description": "This example demonstrates how to implement a GraphQL server with an email-password-based authentication workflow based on Prisma & graphql-yoga.",
  "main": "index.js",
  "repository": "https://github.com/prismagraphql/prisma/tree/master/examples",
  "author": "Prisma",
  "license": "Apache 2",
  "scripts": {
    "start": "cd src && ts-node index.ts",
    "migrate": "cd prisma && prisma deploy",
    "test": "jest",
    "test_reset_seed": "yarn test_reset && yarn test_seed",
    "test_reset": "cd __tests__/test_fixtures/ && prisma reset -f",
    "test_seed": "ts-node __tests__/seed.ts"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^7.2.8",
    "bcrypt": "^3.0.0",
    "graphql": "^0.13.2",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.16.1",
    "jsonwebtoken": "^8.3.0",
    "prisma-client-lib": "1.19.0-beta.2"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/jest": "^23.3.5",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "ts-node": "7.0.1",
    "typescript": "3.1.3"
  }
}